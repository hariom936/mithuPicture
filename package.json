{
  "name": "adani_assement",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && cpx 'appData/img/**/*' 'build/appData/img '",
    "start": "ts-node src/index.ts",
    "start:local": "env-cmd -f ./.env.local ts-node index.ts",
    "start:development": "env-cmd -f ./.env.dev ts-node index.ts",
    "serve": "nodemon --watch 'src/**/*.ts' --watch 'src/**/*.json' --watch index.ts --exec ts-node index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check **/*.ts",
    "format:fix": "prettier --write **/*.ts",
    "debug": "node --inspect index.js",
    "seed": "env-cmd npx ts-node ./scripts/populateDB.ts up",
    "seed:down": " env-cmd npx ts-node ./scripts/populateDB.ts down",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/config/dbconfig.ts",
    "migration:revertLatest": "typeorm-ts-node-commonjs migration:revert -d ./src/config/dbconfig.ts"
  },
  "keywords": [],
  "author": "Kelltontech",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "0.3.20",
    "typescript": "^5.4.5",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.13",
    "cpx": "^1.5.0",
    "ts-node": "^10.9.2"
  }
}
